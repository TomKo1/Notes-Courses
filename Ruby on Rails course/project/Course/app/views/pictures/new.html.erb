<div class="hero-unit">
<h1>New Picture</h1>

<%= form_for(:pictures, :url => {:action => 'create', :gallery_id =>  @galleryOne.id} ) do |field| %>

<%= render(:partial => "form", :locals => {:field => field}) %>

<div style="text-align: center">
<%= link_to("Return", {:action => 'index', :gallery_id =>  @galleryOne.id}, :class => 'btn btn-warning') %>
<%= submit_tag("Create", :class => 'btn btn-primary') %>
</div>

<% end %>
</div>


